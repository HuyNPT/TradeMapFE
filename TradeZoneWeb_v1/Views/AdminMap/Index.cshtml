                    <div class="pcoded-content">
                        <div class="pcoded-inner-content">
                            <div class="main-body">
                                <div id="map" style="z-index: 0"></div>
                                <script type="text/javascript" src="~/default/assets/js/admin-map-init.js"></script>
                                <!-- Button show location -->
                                <button class="btn btn-primary btn-icon"
                                        id="btnShowLocation"
                                        style="
                      position: absolute;
                      bottom: 20px;
                      left: 20px;
                      border-top: none !important;
                    ">
                                    <i class="icofont icofont-location-arrow"></i>
                                </button>
                                <!-- Store list -->
                                <button class="btn btn-primary btn-icon"
                                        style="
                      position: absolute;
                      bottom: 20px;
                      left: 70px;
                      border-top: none !important;
                      visibility: hidden;
                    "
                                        data-toggle="modal"
                                        data-target="#addLocationModal">
                                    <i class="icofont icofont-ui-add" style="margin-right: 0px !important" id="newZone"></i>
                                </button>
                                <div class="modal fade" id="addLocationModal">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <form asp-controller="AdminMap" asp-action="AddNew">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="exampleModalLabel">
                                                        Create new
                                                    </h5>
                                                    <button type="button"
                                                            class="close"
                                                            data-dismiss="modal"
                                                            aria-label="Close"
                                                            id="closeCreateNewButton">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                                <div class="modal-body">
                                                    <div class="form-group row">
                                                        <label class="col-sm-2 col-form-label">Name</label>
                                                        <div class="col-sm-10">
                                                            <input type="hidden" asp-for="WardId" id="wardId"/>
                                                            <input type="hidden" asp-for="Wkt" id="wkt" />
                                                            <input type="text" asp-for="Name"
                                                                   class="form-control"
                                                                   id="nameCreateNew" />
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-sm-2 col-form-label">Ward</label>
                                                        <div class="col-sm-10">
                                                            <input type="text"
                                                                   class="form-control" readonly id="wardName" />
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-sm-2 col-form-label">District</label>
                                                        <div class="col-sm-10">
                                                            <input type="text"
                                                                   class="form-control" readonly id="districtName" />
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-sm-2 col-form-label">Type</label>
                                                        <div class="col-sm-10">
                                                            <select asp-for="Type"
                                                                    class="form-control form-control-default"
                                                                    id="typeCreateNew">
                                                                <option value="systemZone">System zone</option>
                                                                <option value="campus">Campus</option>
                                                                <option value="streetSeqment">Street seqment</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="modal-footer">
                                                    <button type="button"
                                                            class="btn btn-secondary"
                                                            data-dismiss="modal"
                                                            id="cancelCreateNewButton">
                                                        Cancel
                                                    </button>
                                                    <button type="submit"
                                                            class="btn btn-primary col-sm-3"
                                                            id="createNewButton">
                                                        Create
                                                    </button>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                <!-- Modal -->
                                <script type="text/javascript" src="~/default/assets/js/admin-map.js"></script>
                            </div>
                        </div>
